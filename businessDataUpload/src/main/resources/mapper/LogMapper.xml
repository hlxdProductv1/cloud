<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hlxd.microcloud.dao.LogMapper">

    <insert id="insertLog" parameterType="com.hlxd.microcloud.entity.Log">
        insert into t_pro_log
        (id,faultTime,message)
        values
        (#{item.id},#{item.faultTime},#{item.message})
    </insert>


</mapper>
